    "img_04": {
        "page": "4",
        "pageWidth": "600",
        "pageHeight": "848",
        "sections": {
            "18": {
                "class": "text",
                "x": 10,
                "y": 37,
                "height": 767,
                "width": 559,
                "leftCorner": [
                    10,
